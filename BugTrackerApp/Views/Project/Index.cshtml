@model IEnumerable<Project>

@{
    ViewData["Title"] = "Index";
}

<h1>Projects</h1>

<table class="table table-bordered table-striped" style="width:100%">
    <thead>
        <tr>
            <th>
                Project Name
            </th>
            <th>
                Project Description
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var obj in Model)
        {
            <tr>
                <td width="50%">
                    @obj.Name
                </td>
                <td width="30%">
                    @obj.Description
                </td>
            </tr>
        }
    </tbody>
</table>

